package com.spzf.model.privilege;

import java.io.Serializable;

import com.spzf.model.BaseModel;

/**
 * @author 
 
 * @date 2014-08-13 11:58:38
 */
public class UserCompany extends BaseModel implements Serializable{
	private static final long serialVersionUID = -6223954368117765720L;

	/**
     * 
     */
    private String id;
    
    /**
     * 用户id
     */
    private String userId;
    
    /**
     * 公司id
     */
    private String companyId;
    
    public String getId()
    {
        return id;
    }
        
    public void setId(String id)
    {
        this.id = id;
    }
        
    public String getUserId()
    {
        return userId;
    }
        
    public void setUserId(String userId)
    {
        this.userId = userId;
    }
        
    public String getCompanyId()
    {
        return companyId;
    }
        
    public void setCompanyId(String companyId)
    {
        this.companyId = companyId;
    }
}
